{
  "Framework": "jasmine",
  "FrameworkVersion": "2",
  "TestFileTimeout": 10000,
  "RootReferencePathMode": "SettingsFileDirectory",
  "vsoutput": true,

  "Compile": {
    "Extensions": [ ".ts" ],
    "ExtensionsWithNoOutput": [ ".d.ts" ],
    "Executable": null,
    "Mode": "External",
    "UseSourceMaps": true
  },

  "References": [
    {
      "Path": "UnitTests",
      "IncludeInTestHarness": true,
      "Includes": [ "markit-UnitTests.js" ]
    },

    {
      "Path": "scripts/snap",
      "IncludeInTestHarness": true,
      "Includes": [ "**.js" ],
      "Excludes": [ "*-build*", "**-min*" ]
    }

  ],
  "Tests": [
    { "Path": "UnitTests" }
  ]
    
}
